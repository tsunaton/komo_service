<h1>Log in</h1>
        <%= form_with url: staff_login_path, local: true do |f| %>
          <div>
            <%= f.label :email %>
            <%= f.text_field :email %>
          </div>
          <div>
            <%= f.label :password %>
            <%= f.password_field :password %>
          </div>
          <%= f.submit "ログイン" %>

          <%= link_to 'まだアカウントがない？', staff_signup_path %>


        <% end %>
