<h1>Editing User</h1>

<%= form_for @user do |f| %>
  <div>
    <%= f.label :氏名 %>
    <%= f.text_field :name %>
  </div>
  <div>
    <%= f.label :アドレス %>
    <%= f.text_field :email %>
  </div>
  <div>
    <%= f.label :住所 %>
    <%= f.text_field :address %>
  </div>

  <%= f.submit "登録申請" %>
<% end %>


<%= link_to '一覧に戻る', oba_users_path %>
